<main>

<% @devs.each do |dev| %>
  <article>
    <%= image_tag (dev.picture_url), {height: 150} %>
    <h4><%= dev.first_name %></h4>
    <h4><%= dev.interested_in %></h4>
    <h4><%= dev.github_username %></h4>
    <h4><%= link_to "click to view user", dev_path(dev) %></h4>
  </article>

<% end %>

</main>
